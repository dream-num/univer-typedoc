{
  "name": "univer-typedoc",
  "type": "module",
  "version": "0.2.0",
  "private": false,
  "scripts": {
    "prepare": "simple-git-hooks",
    "typedoc": "cd submodules/univer && pnpm i && cd ../../ && node ./scripts/pre-typedoc.mjs && typedoc && node ./scripts/post-typedoc.mjs",
    "dev": "next dev",
    "build": "NODE_OPTIONS=--max-old-space-size=12288 next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "next": "14.2.4",
    "nextra": "^2.13.4",
    "nextra-theme-docs": "^2.13.4",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.1",
    "@eslint-react/eslint-plugin": "^1.5.18",
    "@types/node": "^20.14.9",
    "autoprefixer": "^10.4.19",
    "eslint": "9.5.0",
    "eslint-plugin-readable-tailwind": "^1.5.2",
    "fs-extra": "^11.2.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.38",
    "sharp": "^0.33.4",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.4",
    "typedoc": "^0.26.3",
    "typedoc-plugin-markdown": "^4.1.0",
    "typescript": "^5.5.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "!submodules/*": "eslint"
  }
}
